<h1>Editing horario</h1>

<% form_for(@horario) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :nombre_especialidad %><br />
    <%= f.text_field :nombre_especialidad %>
  </p>
  <p>
    <%= f.label :dias %><br />
    <%= f.text_field :dias %>
  </p>
  <p>
    <%= f.label :horas %><br />
    <%= f.text_field :horas %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @horario %> |
<%= link_to 'Back', horarios_path %>